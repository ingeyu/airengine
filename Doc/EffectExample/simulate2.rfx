     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D Texture0;
float fDistance;
float fCount;
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
   float2 xy   =   texCoord*float2(2,-2)+float2(-1,1);

   float2 pixel   =   1.0f/float2(fCount,1.0f);
   float2 halfpixel   =   0.5f*pixel;
   //float2 uv   =   halfpixel+texCoord;
   for(int i=0;i<8;i++){
      float4 uv   =   float4((i+0.5)/fCount,0.5,0,0);
