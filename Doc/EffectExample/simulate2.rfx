     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float2 fMouseCoordsXY_NDC;
float fMouseButtonStateLeft;
float fTimeDelta;
sampler2D Texture0;
float fDistance;
float fCount;
float2   G;
float4 calc(float2 force,float timedelta,float4 init,float4 parent){
      float4 c   =   init;

      float2 a   =   force;
      float2 offset   =   c.zw*timedelta+0.5*a*timedelta*timedelta;
      
      c.xy+=offset;
      c.zw+=a*fTimeDelta;
         
      if(distance(c.xy+offset,parent.xy)   > fDistance){
         float2 last   =   c.xy;
         float2   olddir   =   c.zw;
         float2 tempdir   =   (c.xy+offset-parent.xy);
         float2 v   =   tempdir.yx*float2(1,-1);
         float2 fixv   =   sign(dot(v,olddir))*v;
         
         c.xy-=tempdir*timedelta*8;
         c.zw=((c.xy-init.xy)/timedelta);
      }
      
      
      return c;
};

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{


   
   float2 pixel   =   1.0f/float2(fCount,1.0f);
   float2 uv   =   texCoord+float2(0.5f/fCount,0.5f);
   float4 c   =   tex2Dlod(Texture0,float4(uv,0,0));
   [branch]
   if(texCoord.x<0.5/fCount){
      if(fMouseButtonStateLeft > 0.5f){
         return float4(fMouseCoordsXY_NDC.x,1-fMouseCoordsXY_NDC.y,0,0);
      }else{
         return c;
      }
   }else if(texCoord.x>(1-0.5f/fCount)){
  
   
      c   =   c*2-1;
      float4 parent   =  tex2Dlod(Texture0,float4(uv+float2(-1,0)*pixel,0,0));
      parent     =   parent*2-1;
      
      float2   FParentDir   =   (parent.xy-c.xy);
      if(length(FParentDir)   < fDistance){
          FParentDir=float2(0,0);
      }else{
          FParentDir   -=   normalize(FParentDir)*fDistance;
      }
      
      float2   Force   =   G+FParentDir;
      
      
      return calc(Force,fTimeDelta,c,parent)*0.5+0.5;
      
   }else{
      
   
      c   =   c*2-1;
      float4 parent   =  tex2Dlod(Texture0,float4(uv+float2(-1,0)*pixel,0,0));
      float4 child    =  tex2Dlod(Texture0,float4(uv+float2(1,0)*pixel,0,0));
      parent     =   parent*2-1;
      child      =   child*2-1;
      
      float2   FParentDir   =   (parent.xy-c.xy);
      if(length(FParentDir)   < fDistance){
          FParentDir=float2(0,0);
      }else{
          //FParentDir   -=   normalize(FParentDir)*fDistance;
      }
      
      float2   FChildDir   =   (child.xy-c.xy);
      if(length(FChildDir)   < fDistance){
         FChildDir=float2(0,0);
      }else{
         //FChildDir   -=   normalize(FChildDir)*fDistance;
      }
      
      float2   Force   =   G+FParentDir+FChildDir;
      float4 pp   =   calc(Force,fTimeDelta,c,parent);
      //float4 pc   =   calc(Force,fTimeDelta,c,child);
      
      return (pp)*0.5+0.5;
   }

}

]]></RmShaderCode>
